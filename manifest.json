{
    "manifest_version": 3,
    "name": "Delta Hacks XI",
    "version": "1.0",
    "description": "For people with ADHD.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab", "http://localhost/*"],
    "content_scripts": [
      {
        "matches":["<all_urls>"],
        "js":["content.js"]
      }
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    }
  }
  