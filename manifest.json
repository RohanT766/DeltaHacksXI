{
    "manifest_version": 3,
    "name": "Delta Hacks XI",
    "version": "1.0",
    "description": "For people with ADHD.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    }
  }
  